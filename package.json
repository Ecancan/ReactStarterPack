  "private": true,
  "main": "index.js",
  "scripts": {
    "start": "webpack serve --config ./webpack/webpack.config.dev.js",
    "build": "webpack --config ./webpack/webpack.config.prod.js",
    "format": "prettier --write './src/**/*.{ts,tsx,js,jsx,json}'",
    "tsc": "tsc",
    "tsc:watch": "tsc --watch",
    "lint": "eslint . --ext .js,.ts,.tsx",
    "lint:fix": "cd \"./src/\" && eslint . --fix --ext .ts,.tsx,.js && cd ..",
    "fix:all": "npm run format && npm run lint:fix",
    "husky-lint-staged": "lint-staged && npm run fix:all && npm run lint && npm run tsc",
    "postinstall": "husky install",
    "talwind:watch": "npx tailwindcss -i ./src/index.css -o ./src/tailwind.css --watch",
    "svgr": "npx @svgr/cli ./src/assets/icons -d src/components/icons --typescript --icon --svg-props \"width=24,height=24\""
  },
